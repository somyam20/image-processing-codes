   "source": [
    "import cv2\n",
    "import numpy as np\n",
    "\n",
    "img = cv2.imread('dilated2.5a.png')\n",
    "median = cv2.medianBlur(img, 7)\n",
    "compare = np.concatenate((img, median), axis=1) #side by side comparison\n",
    "\n",
    "cv2.imshow('img', compare)\n",
    "cv2.imwrite('dilmederos2.510.png',median)\n",
    "cv2.waitKey(0)"
   ]
