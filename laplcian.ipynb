   "source": [
    "import cv2\n",
    "\n",
    "# Load image in grayscale\n",
    "image = cv2.imread('fingray.png', 0)\n",
    "\n",
    "# Apply Laplacian edge detection\n",
    "edges = cv2.Laplacian(image, cv2.CV_64F)\n",
    "\n",
    "# Display the result\n",
    "cv2.imshow('Laplacian edges', edges)\n",
    "cv2.waitKey(0)"
   ]
  },
